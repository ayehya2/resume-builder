{
  "name": "resume-builder",
  "version": "1.0.0",
  "description": "Professional Resume Builder with LaTeX PDF generation",
  "main": "backend/app/app.js",
  "scripts": {
    "start": "node backend/app/app.js",
    "dev": "nodemon backend/app/app.js",
    "dev:open": "OPEN_BROWSER=true nodemon backend/app/app.js",
    "start:open": "OPEN_BROWSER=true node backend/app/app.js",
    "build": "npm install && PUPPETEER_CACHE_DIR=$PWD/.puppeteer_cache npx puppeteer browsers install chrome && pip install -r requirements.txt",
    "install-chromium": "npx puppeteer browsers install chrome",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "kill": "scripts\\kill-port.bat",
    "kill:port": "scripts\\kill-port.bat",
    "restart": "scripts\\restart.bat",
    "restart:dev": "scripts\\restart.bat dev"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.56.0",
    "archiver": "^5.3.2",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "common-tags": "^1.8.2",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.6.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "glob": "^10.3.10",
    "helmet": "^7.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "open": "^9.1.0",
    "puppeteer": "^23.9.0",
    "python-shell": "^5.0.0",
    "uuid": "^11.1.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "keywords": [
    "job-search",
    "nodejs",
    "python",
    "express",
    "web-scraping"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}